    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "ProducerApplication",
            "request": "launch",
            "mainClass": "com.example.Producer.ProducerApplication",
            "projectName": "Producer"
        },
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "FHIRHl7KafkaConsumerApplication",
            "request": "launch",
            "mainClass": "com.kafka.consumer.FHIRHl7KafkaConsumerApplication",
            "projectName": "FHIRHl7KafkaConsumer"
        },
        {
            "type": "java",
            "name": "FHIRHl7KafkaProviderApplication",
            "request": "launch",
            "mainClass": "com.kafka.provider.FHIRHl7KafkaProviderApplication",
            "projectName": "FHIRHl7KafkaProvider"
        },
        {
            "type": "java",
            "name": "SpringBootHapiFhirApplication",
            "request": "launch",
            "mainClass": "com.spring.hapi.fhir.springboothapifhir.SpringBootHapiFhirApplication",
            "projectName": "spring-boot-hapi-fhir"
        }
    ]
}